{
  "name": "riordan-calculator",
  "version": "0.0.0",
  "private": true,
  "author": "James Shapiro <james@jamesshapiro.com>",
  "dependencies": {
    "@aws-amplify/auth": "^6.0.27",
    "@aws-amplify/ui-react": "^6.1.8",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "aws-amplify": "^6.0.28",
    "eslint": "8.49.0",
    "eslint-config-react-app": "7.0.1",
    "framer-motion": "^11.1.1",
    "howler": "^2.2.4",
    "new-component": "^5.0.2",
    "parcel": "2.9.2",
    "process": "0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.1",
    "spinners-react": "^1.0.7",
    "styled-components": "^6.1.8",
    "use-sound": "^4.0.1"
  },
  "alias": {
    "howler": "howler/dist/howler.core.min.js"
  },
  "scripts": {
    "predev": "rimraf .parcel-cache dist",
    "dev": "parcel public/index.html",
    "build": "parcel build public/index.html",
    "new-component": "new-component",
    "build:upload-to-aws": "aws s3 sync dist s3://riordan-demo.weakerpotions.com --delete --size-only && aws s3 cp dist/index.html s3://riordan-demo.weakerpotions.com/index.html",
    "build:invalidate-cf-cache": "aws cloudfront create-invalidation --distribution-id E14BH8WEKGBSJ --paths '/*' --no-cli-pager",
    "build:clean-build-files": "rm -rf dist/",
    "postbuild": "npm run build:upload-to-aws && npm run build:invalidate-cf-cache && npm run build:clean-build-files"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "buffer": "^6.0.3",
    "prettier": "^3.2.5"
  }
}
